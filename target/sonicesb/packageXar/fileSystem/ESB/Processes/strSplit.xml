    <xq:itinerary>
        <xq:step description="Split string" endpoint_ref="StringOpsServiceInstance" name="Service 1" type="SERVICE">
            <xq:params>
                <xq:stringParam name="operationId">StringOpsService.split</xq:stringParam>
