<?xml version="1.0" encoding="UTF-8"?>
<xq:process name="strSplit" preserveMessageOrder="false" processModelVersion="3.0" qualityOfService="BEST_EFFORT" timeToLive="0" xmlns:xq="http://www.sonicsw.com/sonicxq" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sonicsw.com/sonicxq file:///C:/Users/JUANM~1.WIN/AppData/Local/Temp/3/mvnesbplugin5649275531523322693219/sonicesb-maven-plugin/ESB2015/schema/process.xsd">
    <xq:exitEndpoint_list>
        <xq:exitEndpoint endpoint_ref="REPLY_TO"/>
    </xq:exitEndpoint_list>
    <xq:faultEndpoint endpoint_ref="REPLY_TO"/>
